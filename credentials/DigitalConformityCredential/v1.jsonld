{
  "@context": {
    "@version": 1.1,
    "schema.org": "https://schema.org/",
    "uncefact-vocab": "https://vocabulary.uncefact.org/",
    "untp-spec": "https://uncefact.github.io/spec-untp/docs/specification/",
    "untp-dcc": "https://test.uncefact.org/vocabulary/untp/cc/",
    "untp-core": "https://test.uncefact.org/vocabulary/untp/core/",
    "DigitalConformityCredential": {
      "@id": "untp-spec:ConformityCredential",
      "@context": {
        "@protected": true
      }
    },
    "ConformityAttestation": {
      "@id": "untp-dcc:ConformityAttestation",
      "@context": {
        "@protected": true,
        "scope": "untp-dcc:scope",
        "issuedTo": "untp-core:issuedToParty",
        "assessments": "untp-dcc:assessments"
      }
    },
    "ConformityAssessment": {
      "@id": "untp-dcc:ConformityAssessment",
      "@context": {
        "@protected": true,
        "products": "untp-dcc:subjectProducts",
        "facilities": "untp-dcc:subjectFacilities",
        "regulation": "untp-core:referenceRegulation"
      }
    },
    "Regulation": {
      "@id": "untp-core:Regulation",
      "@context": {
        "@protected": true
      }
    }
  }
}
